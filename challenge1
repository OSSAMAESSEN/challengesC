 #include <stdio.h>
#include <stdlib.h>
int main()
{
    char nom[20];
    char prenom[20];
    int age;
    char sexe;
    int numeroDeTelephone;

    printf("quelle votre nom? \n");
    scanf("%s", nom);

    printf("quelle votre prenom? \n");
    scanf("%s", prenom);

    printf("quelle votre age? \n");
    scanf("%d", &age);

    printf("quelle votre sexe? \n");
    scanf("%s", sexe);

    printf("quelle votre numeroDeTelephone? \n");
    scanf("%d", &numeroDeTelephone);

    return 0;

    }
