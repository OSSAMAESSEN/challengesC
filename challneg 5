  #include <stdio.h>
#include<stdlib.h>
 
int main()
{
   float Fahrenheit, Celsius;
   printf("Fahrenheit=");
   scanf("%f",&Fahrenheit);
   Celsius = (Fahrenheit-32)/1.8;
   printf("%f",Celsius);
 if (celsius<= 0){
                printf("très froid");

     }
        if (celsius>= 0){
                printf("froid");

     }
       if (celsius>20){
                printf("chod");

     }
     if (celsius>30){
                printf("très chod");

     }
}
      
